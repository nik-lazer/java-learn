<?xml version="1.0" encoding="UTF-8"?>
<module rename-to="GwtWeb">
  <!-- Inherit our applications main module.                      -->
  <inherits name='lan.training.gwt.GwtWeb' />

  <!-- Specify the path to any remote services.                   -->
  <servlet path="/GwtWeb/books" class="lan.training.gwt.server.BookRemoteServiceImpl" />
	<servlet path="/GwtWeb/authors" class="lan.training.gwt.server.AuthorRemoteServiceImpl" />
	<servlet path="/GwtWeb/publishers" class="lan.training.gwt.server.PublisherRemoteServiceImpl" />
	<servlet path="/GwtWeb/languages" class="lan.training.gwt.server.LanguageRemoteServiceImpl" />

</module>
